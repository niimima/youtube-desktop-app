<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="PlaylistEditor.Views.SearchBox">
	<StackPanel Orientation="Horizontal">
		<Button Name="m_SearchButton" HorizontalAlignment="Center" 
				Command="{Binding Search}" CommandParameter="{Binding ElementName=m_SearchTextBox, Path=Text}">
			Search Button
		</Button>
		<TextBox Name="m_SearchTextBox" Height="20" Width="200" Watermark="Search Word" />
	</StackPanel>
</UserControl>
